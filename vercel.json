{
  "version": 2,
  "builds": [
    { "src": "main.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api", "dest": "main.py" },
    { "src": "/api/", "dest": "main.py" },
    { "src": "/", "dest": "main.py" }
  ],
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  }
}
